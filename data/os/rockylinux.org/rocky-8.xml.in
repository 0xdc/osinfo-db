        <system-id>LINUX</system-id>
        <volume-id>Rocky-8-([4-9]|[1-9][0-9])-aarch64-.+$</volume-id>
      </iso>
      <kernel>images/pxeboot/vmlinuz</kernel>
      <initrd>images/pxeboot/initrd.img</initrd>
    </media>

