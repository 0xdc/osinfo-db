    <media arch="x86_64">
      <iso>
        <system-id>LINUX</system-id>
        <volume-id>Rocky-8-([4-9]|[1-9][0-9])-x86_64-.+$</volume-id>
      </iso>
      <kernel>isolinux/vmlinuz</kernel>
      <initrd>isolinux/initrd.img</initrd>
    </media>

    <media arch="aarch64">
